<div class="container">

  <%# TODO: 
  1. If user is not signed in - display "Welcome".
  2. If user is signed in then  display all users %>

   

  <% if current_user %>
    <span><a href='/sessions/<%= @current_user.id %>'>Sign Out</a></span>
    <h3>
      Below are all users:
    </h3>
    
    <ul>
    <% @users.each do |user| %>
      <li>
        <%= user.name %>
      </li>
    <% end %>
    </ul>
  <% end %>

  <% unless current_user %>

  <h1> Welcome </h1>
  
  <% end %>
</div>
